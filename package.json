{
  "name": "capstone",
  "version": "2.0.0",
  "description": "An app to encourage people to go outside through challenges",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "SEED=true node server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "CEJS",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@faker-js/faker": "^7.2.0",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@reach/combobox": "^0.17.0",
    "@react-google-maps/api": "^2.12.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "google-map-react": "^2.1.10",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
    "react-google-login": "^5.2.2",
    "react-google-places-autocomplete": "^3.3.4",
    "sequelize": "^6.3.5",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "timeago.js": "^4.0.2",
    "use-places-autocomplete": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2"
  }
}
